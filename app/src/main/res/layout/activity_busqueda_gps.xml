<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawer_layout_gps"
    tools:context="co.edu.udea.cmovil.gr02.whereverent.BusquedaGpsActivity">
    //Layout que permite visualizar el GoogleMap
    <com.google.android.gms.maps.MapView
        android:id="@+id/mi_mapa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    //Se incluye la listView que contiene la informacion del navigation drawer
    <include layout="@layout/list_navigation"></include>
</android.support.v4.widget.DrawerLayout>
